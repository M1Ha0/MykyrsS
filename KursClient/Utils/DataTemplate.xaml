<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:KursClient.ViewModels"
                    xmlns:view="clr-namespace:KursClient.Views">
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:FlightViewModel}">
        <view:FlightView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PassengerViewModel}">
        <view:PassengerView/>
    </DataTemplate>
</ResourceDictionary>